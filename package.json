{
  "name": "slye",
  "productName": "slye",
  "author": "Parsa Ghadimi <qti3eqti3e@gmail.com>",
  "version": "0.0.0",
  "description": "Slye is an open-source 3D presentation software",
  "scripts": {
    "lint": "tslint --project .",
    "build": "rm -rf build && npm run build:main && npm run build:renderer",
    "build:renderer": "parcel build src/renderer/app.html -d ./build/renderer --public-url ./",
    "build:main": "tsc --outDir build --p './tsconfig.json'",
    "start": "electron build/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Slye3D/slye.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Slye3D/slye/issues"
  },
  "keywords": [],
  "homepage": "https://github.com/Slye3D/slye#readme",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "electron": "^1.8.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0",
    "parcel-bundler": "^1.6.2"
  },
  "dependencies": {
    "es6-tween": "^5.1.0",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "jquery.nicescroll": "^3.7.6",
    "material-ui": "^0.20.0",
    "mkdirp": "^0.5.1",
    "nice-color-palettes": "^2.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-drag-sortable": "^1.0.6",
    "react-router-dom": "^4.2.2",
    "tar": "^4.4.1",
    "three": "^0.91.0",
    "tmp": "0.0.33",
    "uuid": "^3.2.1"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
